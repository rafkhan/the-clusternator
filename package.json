{
  "name": "clusternator",
  "version": "0.5.8",
  "description": "Program to deploy docker containers on cloud service platforms with minimal user input, and/or configuration",
  "main": "index.js",
  "preferGlobal": true,
  "keywords": [
    "docker",
    "aws",
    "amazon",
    "cluster",
    "cloud",
    "heroku",
    "vm",
    "virtual machine",
    "container"
  ],
  "bugs": {
    "url": "https://github.com/rangle/the-clusternator/issues"
  },
  "os": [
    "!win32"
  ],
  "scripts": {
    "build": "gulp transpile",
    "dev": "NODE_ENV=debug; concurrent \"nodemon ./server-init.js\" \"npm run watch\"",
    "doc": "npm run doc-api; jekyll build -s ./docs -d ./docs/_site",
    "doc-api": "jsdoc -c ./js-doc.json",
    "doc-dev": "concurrent \"npm run doc-dev-serve\" \"npm run watch\"",
    "doc-dev-serve": "jekyll serve -s ./docs -d ./docs/_site --drafts",
    "doc-publish": "./bin/publish-docs.sh",
    "graph": "echo 'Requires GraphViz (If it is not already installed)'; madge --exclude \"node_modules|bin-src|lib|path|spec|coverage|tmp|examples\"  --image coverage/clusternator-graph.png ./",
    "lint": "gulp lint",
    "prepublish": "npm run doc-publish; npm run build",
    "staging": "NODE_ENV=staging; node ./server-init.js",
    "start": "node ./server-init.js",
    "test": "gulp test-unit",
    "watch": "gulp watch",
    "release": "git commit -am $npm_package_version && git tag $npm_package_version && git push && npm publish && git push --tags"
  },
  "homepage": "http://the-clusternator.rangle.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/the-clusternator"
  },
  "author": "rangle.io",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.3.9",
    "base64url": "^1.0.5",
    "body-parser": "^1.14.2",
    "body-parser-rawbody": "^1.13.2",
    "compression": "^1.6.0",
    "connect-ensure-login": "^0.1.1",
    "credential": "^0.2.6",
    "deepmerge": "^0.2.10",
    "express": "^4.13.3",
    "express-winston": "^1.3.1",
    "gulp-util": "^3.0.7",
    "inquirer": "^1.0.0",
    "media-typer": "^0.3.0",
    "mkdirp": "^0.5.1",
    "parse-git-config": "^0.3.1",
    "passport": "^0.3.2",
    "passport-http-header-token": "^1.0.0",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "ramda": "^0.21.0",
    "request": "^2.72.0",
    "rimraf": "^2.4.3",
    "superagent": "^1.8.3",
    "winston": "^1.1.2",
    "yamljs": "^0.2.4",
    "yargs": "^4.7.0"
  },
  "bin": {
    "clusternator": "./bin/clusternator.sh"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "chai-as-promised": "^5.3.0",
    "concurrently": "^1.0.0",
    "eslint": "^2.9.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "gulp-eslint": "^1.0.0",
    "gulp-exit": "0.0.2",
    "gulp-istanbul": "^0.10.4",
    "gulp-mocha": "^2.1.3",
    "gulp-plumber": "^1.0.1",
    "gulp-util": "^3.0.7",
    "jsdoc": "^3.4.0",
    "madge": "^0.5.3",
    "mocha": "^2.3.3",
    "mock-fs": "^3.9.0",
    "mock-spawn": "^0.2.6",
    "nodemon": "^1.9.2",
    "rewire": "^2.3.4",
    "sinon": "^1.17.4",
    "supertest": "^1.1.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "generators": true
    },
    "rules": {
      "comma-dangle": 0,
      "max-len": [
        1,
        80,
        4
      ],
      "no-console": 2,
      "no-func-assign": 2,
      "no-multi-spaces": 0,
      "no-undef": 2,
      "no-underscore-dangle": 0,
      "no-unreachable": 2,
      "no-var": 2,
      "new-cap": 0,
      "one-var": [
        2,
        "never"
      ],
      "quotes": [
        1,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "valid-typeof": 2
    }
  }
}
